#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/graphql-language-service@4.1.4_graphql@16.1.0/node_modules/graphql-language-service/dist/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/graphql-language-service@4.1.4_graphql@16.1.0/node_modules/graphql-language-service/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/graphql-language-service@4.1.4_graphql@16.1.0/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules:/home/workspace/new-project/graphQlassignment/node_modules:/home/workspace/new-project/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/graphql-language-service/dist/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/graphql-language-service/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/graphql-language-service@4.1.4_graphql@16.1.0/node_modules/graphql-language-service/dist/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/graphql-language-service@4.1.4_graphql@16.1.0/node_modules/graphql-language-service/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/graphql-language-service@4.1.4_graphql@16.1.0/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules:/home/workspace/new-project/graphQlassignment/node_modules:/home/workspace/new-project/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/graphql-language-service/dist/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/graphql-language-service/node_modules"
fi
if [ -x "$basedir//bin/node" ]; then
  exec "$basedir//bin/node"  "$basedir/../graphql-language-service/dist/temp-bin.js" "$@"
else
  exec /bin/node  "$basedir/../graphql-language-service/dist/temp-bin.js" "$@"
fi
