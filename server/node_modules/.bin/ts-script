#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules/ts-node/dist/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules/ts-node/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules:/home/workspace/new-project/graphQlassignment/node_modules:/home/workspace/new-project/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/ts-node/dist/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/ts-node/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules/ts-node/dist/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules/ts-node/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules:/home/workspace/new-project/graphQlassignment/node_modules:/home/workspace/new-project/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/ts-node/dist/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/ts-node/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-script-deprecated.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-script-deprecated.js" "$@"
fi
