#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/markdown-it@12.3.0/node_modules/markdown-it/bin/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/markdown-it@12.3.0/node_modules/markdown-it/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/markdown-it@12.3.0/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules:/home/workspace/new-project/graphQlassignment/node_modules:/home/workspace/new-project/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/graphiql@1.5.16_graphql@16.1.0/node_modules/graphiql/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/graphiql@1.5.16_graphql@16.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/markdown-it@12.3.0/node_modules/markdown-it/bin/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/markdown-it@12.3.0/node_modules/markdown-it/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/markdown-it@12.3.0/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules:/home/workspace/new-project/graphQlassignment/node_modules:/home/workspace/new-project/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/graphiql@1.5.16_graphql@16.1.0/node_modules/graphiql/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/graphiql@1.5.16_graphql@16.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../markdown-it@12.3.0/node_modules/markdown-it/bin/markdown-it.js" "$@"
else
  exec node  "$basedir/../../../../../markdown-it@12.3.0/node_modules/markdown-it/bin/markdown-it.js" "$@"
fi
