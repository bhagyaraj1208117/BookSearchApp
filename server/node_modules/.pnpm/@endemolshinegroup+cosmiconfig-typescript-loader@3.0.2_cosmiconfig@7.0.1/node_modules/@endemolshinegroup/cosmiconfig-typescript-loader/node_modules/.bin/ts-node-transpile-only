#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules/ts-node/dist/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules/ts-node/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules:/home/workspace/new-project/graphQlassignment/node_modules:/home/workspace/new-project/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/@endemolshinegroup+cosmiconfig-typescript-loader@3.0.2_cosmiconfig@7.0.1/node_modules/@endemolshinegroup/cosmiconfig-typescript-loader/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/@endemolshinegroup+cosmiconfig-typescript-loader@3.0.2_cosmiconfig@7.0.1/node_modules/@endemolshinegroup/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/@endemolshinegroup+cosmiconfig-typescript-loader@3.0.2_cosmiconfig@7.0.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules/ts-node/dist/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules/ts-node/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/ts-node@9.1.1/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules:/home/workspace/new-project/graphQlassignment/node_modules:/home/workspace/new-project/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/@endemolshinegroup+cosmiconfig-typescript-loader@3.0.2_cosmiconfig@7.0.1/node_modules/@endemolshinegroup/cosmiconfig-typescript-loader/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/@endemolshinegroup+cosmiconfig-typescript-loader@3.0.2_cosmiconfig@7.0.1/node_modules/@endemolshinegroup/node_modules:/home/workspace/new-project/graphQlassignment/server/node_modules/.pnpm/@endemolshinegroup+cosmiconfig-typescript-loader@3.0.2_cosmiconfig@7.0.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../ts-node@9.1.1/node_modules/ts-node/dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../../../../../../ts-node@9.1.1/node_modules/ts-node/dist/bin-transpile.js" "$@"
fi
